{
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "javascriptreact", "elixir", "markdown", "c"],
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "expr:GetDeleteAction"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-t>": "action:tabe",
    "<C-w>": "command:wincmd k"
  },
  "diagnostic.enable": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "signature.hideOnTextChange": true,
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.floatEnable": false,
  "prettier.statusItemText": "",
  "octobox.iconForStatusBar": "",
  "octobox.showInStatusBar": true,
  "languageserver": {
    "elixirLS": {
      "command": "/Users/gabrielpoca/.elixir_ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
         "cache": {
           "directory": "/tmp/ccls"
         }
       }
    }
  }
}
